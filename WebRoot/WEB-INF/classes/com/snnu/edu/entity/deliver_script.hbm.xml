<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
<class name="com.snnu.edu.entity.Deliver_Script" table="deliver_script">
  <id name="id">
    <generator class="native"></generator>
  </id>
  <property name="time"></property>
  <many-to-one name="manuscript" class="com.snnu.edu.entity.Manuscript" column="fk-ds-ms" lazy="false"></many-to-one>
  <many-to-one name="user" class="com.snnu.edu.entity.User" column="fk-ds-u" lazy="false"></many-to-one>
</class>
	
</hibernate-mapping>