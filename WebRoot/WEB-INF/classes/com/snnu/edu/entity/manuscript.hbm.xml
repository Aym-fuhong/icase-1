<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.snnu.edu.entity.Manuscript" table="manuscript">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="mnamec" length="50"></property>
		<property name="mnamee" length="50"></property>
		<property name="typec" length="50"></property>
		<property name="typee" length="50"></property>
		<property name="titlec" length="50"></property>
		<property name="titlee" length="50"></property>
		<property name="summaryc" length="50"></property>
		<property name="summarye" length="50"></property>
		<property name="keywordc" length="50"></property>
		<property name="keyworde" length="50"></property>
	</class>

</hibernate-mapping>